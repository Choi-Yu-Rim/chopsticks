{
  "manifest_version": 3,
  "name": "젓가락(樂) Connector",
  "version": "2.0",
  "permissions": [
    "storage",
    "nativeMessaging"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.spooncast.net/*"],
      "js": ["content.js"]
    }
  ]
}